<h1>Editar Usuario</h1>
<h1>Editar Usuario</h1>
<% if !object_id %>
<h2><%= pluralize(object.errors.count, "error") %> prohibited this record from being saved:</h2>
<% else %>
  <%= form_with model: @user, url: user_path(@user), method: :patch do |form| %>
    <%= render "shared/error_messages", object: @user %>
    <div class="field">
  <%= form.label :fullName %>
  <%= form.text_field :fullName, value: @user.fullName %>
</div>

<div class="field">
  <%= form.label :userName %>
  <%= form.text_field :userName, value: @user.userName %>
</div>

<div class="field">
  <%= form.label :age %>
  <%= form.number_field :age, value: @user.age %>
</div>

<div class="field">
  <%= form.label :address %>
  <%= form.text_field :address, value: @user.address %>
</div>

<div class="field">
  <%= form.label :email %>
  <%= form.email_field :email, value: @user.email %>
</div>

<div class="field">
  <%= form.label :password_digest %>
  <%= form.password_field :password_digest %>
</div>

<div class="field">
  <%= form.label :password_confirmation %>
  <%= form.password_field :password_confirmation %>
</div>

<div class="actions">
  <%= form.submit "Actualizar Usuario" %>
</div>
  <% end %>
<% end %>